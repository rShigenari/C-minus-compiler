int SomaVet (int a[], int n){
  int i;
  int soma;
  soma = 0;
  i = 0;
  while (i<n){
    soma = a[i] + soma;
    i = i + 1;
  }
  return soma;
}

void main (void){
  int n;
  int i;
  int vet [4];
  n = 4;
  i = 0;
  while (i <n){
    vet[i] = input();
    i = i + 1;
  }
  output (SomaVet(vet, n));
}
